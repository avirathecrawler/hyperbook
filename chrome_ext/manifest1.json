{
   "background": {
      "scripts": [ "js/jquery.min.js", "js/background.js" ]
   },

   // "chrome_url_overrides": {
   //   "newtab": "tabpage.html"
   //},

     

   "permissions": [ "idle", "tabs", "contextMenus", "bookmarks", "http://*.getbook.co/*", "topSites", "chrome://favicon/"  ],
   
   "browser_action": {
      "default_icon": "images/insights.png",
      "default_popup": "popup.html",
