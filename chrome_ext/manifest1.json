      "128": "images/insights.png"
   },
   "manifest_version": 2,
   "name": "HyperBook",
   "optional_permissions": [ "topSites" ],
   //"options_page": "http://getbook.co",
   
   "short_name": "Hyperbook - information organized",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "15.3"
}
